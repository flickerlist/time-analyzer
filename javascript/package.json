{
  "name": "time-analyzer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "gen-parser": "antlr4ts -visitor lib/grammar/TimeLexer.g4 && antlr4ts -visitor lib/grammar/TimeParser.g4",
    "pregen-parser": "yarn clean-parser",
    "clean-parser": "rm -rf lib/grammar/*.interp lib/grammar/*.tokens lib/grammar/*.js lib/grammar/*.tokens lib/grammar/*.ts",
    "test": "jest"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "antlr4": "^4.9.2",
    "antlr4ts": "~0.5.0-alpha.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "antlr4ts-cli": "~0.5.0-alpha.4",
    "jest": "^27.2.2",
    "ts-jest": "^27.0.5",
    "ts-transform-paths": "^2.0.3",
    "typescript": "^4.4.3"
  }
}
