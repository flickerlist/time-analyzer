{
  "name": "time-analyzer",
  "version": "1.3.0",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name" : "Lanistor" ,
    "email" : "lanistor@163.com",
    "url" : "https://github.com/lanistor"
  },
  "repository": {
    "type" : "git",
    "url" : "http://github.com/flickerlist/time-analyzer.git",
    "directory": "javascript"
  },
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.build.json --declaration",
    "gen-parser": "antlr4ts -visitor src/grammar/TimeLexer.g4 && antlr4ts -visitor src/grammar/TimeParser.g4",
    "pregen-parser": "yarn clean-parser",
    "postbuild": "cp README.md package.json lib/ && rm -rf lib/*tsbuildinfo",
    "clean-parser": "rm -rf src/grammar/*.interp src/grammar/*.tokens src/grammar/*.js src/grammar/*.tokens src/grammar/*.ts",
    "test": "jest"
  },
  "keywords": [
    "time",
    "date",
    "parse",
    "parse time",
    "analyzer",
    "nlp"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "antlr4": "^4.9.2",
    "antlr4ts": "~0.5.0-alpha.4",
    "nzh": "^1.0.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "antlr4ts-cli": "~0.5.0-alpha.4",
    "jest": "^27.2.2",
    "ts-jest": "^27.0.5",
    "ts-transform-paths": "^2.0.3",
    "typescript": "^4.4.3"
  }
}
